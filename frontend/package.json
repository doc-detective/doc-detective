{
  "name": "doc-detective-gui",
  "version": "2.5.1-dev.0",
  "description": "Frontend for Doc Detective.",
  "scripts": {
    "build": "react-scripts build",
    "watch": "nodemon --watch ./ --ignore build/* --ext js,mjs,cjs,json,css,html --exec \"npm run start\"",
    "start": "cd ../server && npm run build",
    "postbuild": "node scripts/postProcessing.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "autoprefixer": "^10.4.18",
    "doc-detective-common": "^1.13.1",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
