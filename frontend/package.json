{
  "name": "doc-detective-gui",
  "version": "2.5.1",
  "description": "Frontend for Doc Detective.",
  "scripts": {
    "build": "react-scripts build",
    "watch": "nodemon --watch ./ --ignore build/* --ext js,mjs,cjs,json,css,html --exec \"npm run start\"",
    "start": "cd ../server && npm run build",
    "postbuild": "node scripts/postProcessing.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "autoprefixer": "^10.4.16",
    "doc-detective-common": "^1.9.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.4",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}