{
  "name": "doc-detective-gui",
  "version": "2.1.0-alpha.2",
  "description": "Frontend for Doc Detective.",
  "scripts": {
    "build": "react-scripts build",
    "watch": "nodemon --watch ./ --ignore build/* --ext js,mjs,cjs,json,css,html --exec \"npm run start\"",
    "start": "cd ../server && npm run build",
    "postbuild": "node scripts/postProcessing.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "autoprefixer": "^10.4.15",
    "doc-detective-common": "^1.3.0",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.3",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
